{
  // 这部分是与 MiraiAPIHTTP 通信的参数
  "mirai": {
    "qq": 1234567890,
    "server": "127.0.0.1",
    "port": 8080,
    "auth_key": "1234567890",
    "enable_ws": false
  },
  // 这部分是控制器设置
  // group一般为监听的群组
  // friend一般为监听的好友
  // trigger一般为触发关键词，$后的部分为参数，更改触发词时请保留，如触发词中含空格也请保留
  "controllers": {
    "sudo": {
      "group": [],
      // 此处admin里的群员才能执行sudo
      "admin": [],
      "trigger": [
        "sudo $cmd $obj"
      ]
    },
    "AntiEro": {
      "group": null,
      "friend": [],
      "admin": 1234567890,
      // 以下三个为百度智能云的参数
      "APP_ID": "your_APP_ID",
      "API_KEY": "your_API_KEY",
      "SECRET_KEY": "your_SECRET_KEY"
    },
    "Help": {
      "group": null,
      "friend": null,
      "trigger": [
        "help"
      ]
    },
    "Translate": {
      "group": null,
      "friend": null,
      // 以下两个为百度翻译API的参数
      "appid": "your_appid",
      "secretKey": "your_secretKey",
      "trigger": [
        "翻译 $from_lang $to_lang $obj"
      ]
    },
    "hhsh": {
      "group": null,
      "friend": null,
      "trigger": [
        "好好说话 $obj"
      ]
    },
    "bangumi": {
      "group": null,
      "friend": null,
      // 此处ban内的群员无法使用bangumi，原为开发时防其他bot误触
      "ban": [],
      "trigger": [
        "看$day番"
      ]
    },
    "bang": {
      "group": null,
      "friend": null,
      // 以下三项为 **http** 代理设置，请注意与其他代理设置不同
      "proxy": true,
      "proxy_server": "server",
      "proxy_port": 1081,
      "trigger": [
        "邦 $server"
      ]
    },
    "SauceNAO": {
      "group": null,
      "friend": null,
      "api_key": "your_api_key",
      "EnableRename": false,
      // 最小匹配相似度，小于这个值搜图返回无结果，注意保留感叹号
      "minsim": "25!",
      "thumbSize": [250,250],
      // 以下两项为代理设置，注意与bang的代理设置不同
      "proxy": false,
      "proxy_dict": {
        "http": "http://server:port/",
        "https": "http://server:port/"
      },
      "processResults": true,
      "trigger": [
        "搜图"
      ]
    },
    "cars": {
      "group": [],
      "friend": null,
      // 以下这项为允许使用r18图片的 **群组** 以及 **私聊的好友**
      "allow_r18": [],
      // 以下两项为代理设置，注意与bang的代理设置不同
      "proxy": false,
      "proxy_dict": {
        "http": "http://server:port/",
        "https": "http://server:port/"
      },
      // source: yml | local | lolicon 图源，请注意：使用local时请尽量确保文件夹内只含jpg, png, bmp, gif格式的图片，并且使用local无法返回作者等详细信息
      "source": "yml",
      // 以下四项为图源的相关设置
      "yml_path": "static/Template.yml",
      // 请务必确保所填位置存在
      "local_dir": "static/normal/",
      "local_r18_dir": "static/R18/",
      // https://api.lolicon.app/
      "lolicon_apikey": "your_lolicon_apikey",
      // 以下这项为是否对local的图片进行压缩
      "is_resize": true,
      // 以下这项为构造幻影坦克时，里图压缩最长边的大小，最好为表图最短边的大小
      "max_length": 1139,
      //以下这项为构造幻影坦克的表图
      "cover_path": "static/mirai2.png",
      "trigger": [
        "开车$mod",
        "爆破$mod"
      ],
      // normal_trigger与r18_trigger必须在trigger中
      "normal_trigger": "开车",
      "r18_trigger": "爆破",
      "gray_trigger": "G",
      "color_trigger": "C"
    },
    "birthday": {
      "group": null,
      "friend": null,
      "trigger": [
        "二刺螈生日"
      ]
    },
    "ISML": {
      "group": null,
      "friend": null,
      "trigger": [
        "打仗"
      ]
    }
  }
}